		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.17.27",
		"mocha": "^7.2.0",
		"tslint": "^5.20.1",
		"typescript": "^3.9.7",
		"vscode": "^1.1.37"
	}
}
