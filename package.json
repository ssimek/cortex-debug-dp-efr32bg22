{
	"publisher": "triaxis",
	"name": "cortex-debug-dp-efr32bg22",
	"displayName": "Cortex-Debug: Device Support Pack - EFR32BG22",
	"description": "This package contains additional support functionality (SVD definitions) for Cortex-Debug for EFR32BG22 devices",
	"repository": {
		"type": "git",
		"url": "https://github.com/ssimek/cortex-debug-dp-efr32bg22.git"
	},
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Debuggers"
	],
	"keywords": [
		"Cortex-Debug",
		"Cortex-Debug Device Support Pack",
		"cortex-m",
		"gdb",
		"debug",
		"embedded"
	],
